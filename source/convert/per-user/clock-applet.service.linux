[Unit]
Description=MATE Clock Factory for %u

[Service]
Type=dbus
# The fact that the MATE Clock Factory server requires an X display at startup is a bug.
EnvironmentDirectory=../../../common/env
EnvironmentDirectory=env
ExecStart=/usr/lib/mate-panel/%p
Restart=no

[Install]
